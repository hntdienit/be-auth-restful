{
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "lib": [
      "es2020"
    ],
    "target": "es2020",
    "module": "commonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": [
      "node"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@functions/*": [
        "./src/functions/*"
      ],
      "@lib/*": [
        "./src/libs/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@controllers/*": [
        "./src/controllers/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
    },
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}